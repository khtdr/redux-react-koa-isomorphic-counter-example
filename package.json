{
  "name": "redux-react-koa-isomorphic-counter-example",
  "version": "1.0.1",
  "description": "Isomorphic port of the redux counter app",
  "main": "lib/server",
  "scripts": {
    "cypress:install": "cypress install",
    "cypress:run": "cypress run --browser chrome",
    "cypress:open": "cypress open",
    "prebuild": "make clean",
    "build": "NODE_ENV=production make -j5 build",
    "prestart": "yarn run build",
    "start": "NODE_ENV=production node --harmony lib/server",
    "dev": "NODE_ENV=development make -j5 webpack-server dev-server watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khtdr/redux-react-koa-isomorphic-counter-example.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "redux",
    "koa",
    "server-side",
    "ssr"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khtdr/redux-react-koa-isomorphic-counter-example/issues"
  },
  "homepage": "https://github.com/khtdr/redux-react-koa-isomorphic-counter-example",
  "dependencies": {
    "babel-runtime": "^5.6.15",
    "cypress": "^3.0.1",
    "isomorphic-fetch": "^2.1.0",
    "koa": "^0.21.0",
    "koa-bodyparser": "^2.0.0",
    "koa-compose": "^2.3.0",
    "koa-route": "^2.4.1",
    "koa-static": "^1.4.9",
    "methods": "^1.1.1",
    "nunjucks": "^3.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "babel-core": "^5.5.8",
    "babel-loader": "^5.2.2",
    "nodemon": "^1.17.5",
    "react-hot-loader": "^4.3.3",
    "webpack": "^1.9.11",
    "webpack-dev-server": "^1.9.0"
  }
}
